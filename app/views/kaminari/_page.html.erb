<% if page.current? %>
  <span class="inline-flex items-center justify-center w-9 h-9 rounded-md bg-blue-600 text-white text-sm font-medium"><%= page %></span>
<% else %>
  <%= link_to page, url, class: "inline-flex items-center justify-center w-9 h-9 rounded-md text-sm font-medium text-gray-700 hover:bg-gray-100" %>
<% end %>
